'.source.js':
  'console.log':
    'prefix': 'log'
    'body': 'console.log($1);'
  'console.log variable':
    'prefix': 'lov'
    'body': 'console.log("$1: ", $1);'
'.source.json':
  '"key" : "object"':
    'prefix': 'ko'
    'body': '"${1:key}": {\n\t$2\n}'
  '"key" : "value"':
    'prefix': 'kv'
    'body': '"${1:key}": "${2:value}"$3'
