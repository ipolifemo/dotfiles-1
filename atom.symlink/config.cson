"*":
  editor:
    showIndentGuide: true
    invisibles: {}
    fontSize: 16
    fontFamily: "Menlo"
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    audioBeep: false
    excludeVcsIgnoredPaths: false
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  welcome:
    showOnStartup: false
  "release-notes":
    viewedVersion: "0.89.0"
  "go-plus": {}
  react: {}
  jshint:
    supportLintingJsx: true
  "bracket-matcher":
    autocompleteSmartQuotes: false
    autocompleteBrackets: false
